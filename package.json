{
  "name": "mcservermanager",
  "version": "1.0.1",
  "description": "Minecraft Server Mangement App",
  "main": "app.js",
  "scripts": {
    "test": "electron .",
    "build": "electron-builder --dir",
    "pack": "electron-packager . 'mcservermanager' --out dist/ --overwrite --icon=./icons/icon32.png",
    "deb64": "electron-installer-debian --src dist/mcservermanager-linux-x64/ --arch amd64 --config deb-package-config.json",
    "dist": "npm run pack && npm run deb64"
  },
  "author": "Dunemask",
  "license": "GPL2.1",
  "devDependencies": {
    "electron": "^10.1.1",
    "electron-builder": "^22.8.0",
    "electron-installer-debian": "^3.1.0"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "rimraf": "^3.0.2",
    "tree-kill": "^1.2.2"
  },
  "build": {
    "appId": "com.dunemask.mcservermanager",
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "icon": "./icon32.png",
      "synopsis": "Minecraft Server Manager build with Electron"
    },
    "icon": "./icon32.png"
  }
}
